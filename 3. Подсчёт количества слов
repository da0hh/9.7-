#include <iostream>
#include <string>

int main() {
   setlocale(LC_ALL, "RU");
   std::string str;
   std::getline(std::cin, str);

   int count = 0;
   if (str.length() > 0 && str[0] != ' ')
       count++;
   for (int i = 1; i < str.length(); ++i) {
       if (str[i-1] == ' ' && str[i] != ' ')
           count++;
   }

   std::cout << count << '\n';
}
