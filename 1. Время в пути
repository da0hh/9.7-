#include <iostream>
#include <string>
#include <limits>
#include <numeric>

using namespace std;

int main() {
    setlocale(LC_ALL, "RU");
    string num; int counterDot = 0, counterNumbers = 0;
    cout << "Type number: "; cin >> num;

    bool match = true;
    for (int i = 0; i < num.length(); i++) {
        if (num[i] == '.') {
            counterDot++;
        }
        if ((num[i] < '1' or num[i] > '9') or counterDot > 1 or num[i] == '+' || num[i] == 'e') {
            match = false;
            break;
        }
    }
    if (match) {
        cout << "Yes";
    }
    else {
        cout << "No";
    }
}
